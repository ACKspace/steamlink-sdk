prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/arm-linux-gnueabihf
includedir=${prefix}/include

Name: libass
Description: LibASS is an SSA/ASS subtitles rendering library
Version: 0.10.0
Requires: 
Requires.private: fontconfig >= 2.2.0, enca, fribidi >= 0.19.0, freetype2 >= 9.6.3
Libs: -L${libdir} -lass 
Libs.private: -lm
Cflags: -I${includedir}
